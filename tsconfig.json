{
    "compileOnSave": true,
    "compilerOptions": {
        "target": "ES2022",
        "module": "CommonJS",
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "inlineSourceMap": true,
        "noImplicitThis": true,
        "stripInternal": true,
        "skipLibCheck": true,
        "pretty": true,
        "declaration": true,
        "removeComments": true,
        "forceConsistentCasingInFileNames": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "useUnknownInCatchVariables": false,
        "jsx": "preserve",
        "jsxImportSource": "vue",
        "esModuleInterop": true,
        "typeRoots": ["./node_modules/@types"],
        "outDir": "./dist",
        "baseUrl": "./",
        "paths": {
            "@server/*": ["./server/*"],
            "@web-main-server/*": ["apps/web-main-server/src/*"],
            "@web-windows-server/*": ["apps/web-windows-server/src/*"],
            "@web-windows-ssr-server/*": ["apps/web-windows-ssr-server/src/*"],
            "@web-client-server/*": ["apps/web-client-server/src/*"],
            "@web-client-ssr-server/*": ["apps/web-client-ssr-server/src/*"]
        }
    },
    "exclude": ["dist", "node_modules"]
}
