{
    "$schema": "https://json.schemastore.org/nest-cli",
    "collection": "@nestjs/schematics",
    "sourceRoot": "apps/web-client-ssr-server/src",
    "generateOptions": {
        "flat": false,
        "spec": false
    },
    "compilerOptions": {
        "deleteOutDir": true,
        "webpack": true,
        "tsConfigPath": "apps/web-client-ssr-server/tsconfig.app.json",
        "assets": [{ "include": "../env/*", "outDir": "./dist/env" }]
    },
    "monorepo": true,
    "root": "apps/web-client-ssr-server",
    "projects": {
        "web-main-server": {
            "type": "application",
            "root": "apps/web-main-server",
            "entryFile": "main",
            "sourceRoot": "apps/web-main-server/src",
            "compilerOptions": {
                "tsConfigPath": "apps/web-main-server/tsconfig.app.json"
            }
        },
        "web-client-ssr-server": {
            "type": "application",
            "root": "apps/web-client-ssr-server",
            "entryFile": "main",
            "sourceRoot": "apps/web-client-ssr-server/src",
            "compilerOptions": {
                "tsConfigPath": "apps/web-client-ssr-server/tsconfig.app.json"
            }
        },
        "web-client-server": {
            "type": "application",
            "root": "apps/web-client-server",
            "entryFile": "main",
            "sourceRoot": "apps/web-client-server/src",
            "compilerOptions": {
                "tsConfigPath": "apps/web-client-server/tsconfig.app.json"
            }
        },
        "web-remote-server": {
            "type": "application",
            "root": "apps/web-remote-server",
            "entryFile": "main",
            "sourceRoot": "apps/web-remote-server/src",
            "compilerOptions": {
                "tsConfigPath": "apps/web-remote-server/tsconfig.app.json"
            }
        },
        "web-remote-ssr-server": {
            "type": "application",
            "root": "apps/web-remote-ssr-server",
            "entryFile": "main",
            "sourceRoot": "apps/web-remote-ssr-server/src",
            "compilerOptions": {
                "tsConfigPath": "apps/web-remote-ssr-server/tsconfig.app.json"
            }
        }
    }
}
